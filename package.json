{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@oliversalzburg/js-utils",
  "version": "0.0.50",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliversalzburg/js-utils.git"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./*.js": "./lib/*.js"
  },
  "files": [
    "./lib/**/*.d.ts",
    "./lib/**/*.js",
    "./lib/**/*.map"
  ],
  "scripts": {
    "build": "yarn run build:tsc && yarn run docs:typedoc",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc --watch",
    "clean": "rm -rf ./_site ./coverage ./dist-area ./lib",
    "docs:typedoc": "typedoc",
    "lint": "yarn run lint:all",
    "lint:all": "yarn run lint:eslint && yarn run lint:prettier && yarn run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "nextversion": "./.scripts/manifest-version.cjs",
    "test": "node $(yarn bin mocha) --enable-source-maps",
    "test:coverage": "c8 --reporter html-spa --reporter text node $(yarn bin mocha) --enable-source-maps",
    "test:inspect": "node $(yarn bin mocha) --inspect",
    "preversion": "yarn run lint && yarn run build && yarn run test",
    "postversion": "git push"
  },
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@droppedcode/typedoc-plugin-copy-assets": "1.0.11",
    "@eslint/js": "9.5.0",
    "@oliversalzburg/eslint-config": "0.0.11",
    "@types/chai": "4.3.16",
    "@types/eslint": "8.56.10",
    "@types/mocha": "10.0.7",
    "@types/node": "20.14.8",
    "@types/web": "0.0.149",
    "c8": "10.1.2",
    "chai": "5.1.1",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "48.4.0",
    "eslint-plugin-tsdoc": "0.3.0",
    "globals": "15.6.0",
    "lint-staged": "15.2.7",
    "mocha": "10.5.1",
    "prettier": "3.3.2",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "typedoc": "0.25.13",
    "typedoc-plugin-extras": "3.0.0",
    "typedoc-plugin-mdn-links": "3.2.1",
    "typescript": "5.4.5",
    "typescript-eslint": "8.0.0-alpha.30"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.3.1"
}
