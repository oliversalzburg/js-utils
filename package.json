{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@oliversalzburg/js-utils",
  "version": "0.0.46",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliversalzburg/js-utils.git"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./*.js": "./lib/*.js"
  },
  "files": [
    "./lib/**/*.d.ts",
    "./lib/**/*.js",
    "./lib/**/*.map"
  ],
  "scripts": {
    "build": "yarn run build:tsc",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc --watch",
    "clean": "rm -rf ./coverage ./lib",
    "docs:typedoc": "typedoc",
    "lint": "yarn run lint:all",
    "lint:all": "yarn run lint:eslint && yarn run lint:prettier && yarn run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "test": "node $(yarn bin mocha) --enable-source-maps",
    "test:coverage": "c8 --reporter html-spa --reporter text node $(yarn bin mocha) --enable-source-maps",
    "test:inspect": "node $(yarn bin mocha) --inspect",
    "preversion": "yarn run lint && yarn run build && yarn run test",
    "postversion": "git push"
  },
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@droppedcode/typedoc-plugin-copy-assets": "1.0.11",
    "@eslint/js": "9.4.0",
    "@types/chai": "4.3.16",
    "@types/eslint": "8.56.10",
    "@types/mocha": "10.0.6",
    "@types/node": "20.14.2",
    "@types/web": "0.0.148",
    "c8": "9.1.0",
    "chai": "5.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "48.2.7",
    "eslint-plugin-tsdoc": "0.3.0",
    "globals": "15.3.0",
    "lint-staged": "15.2.5",
    "mocha": "10.4.0",
    "prettier": "3.3.1",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "typedoc": "0.25.13",
    "typedoc-plugin-extras": "3.0.0",
    "typedoc-plugin-mdn-links": "3.1.28",
    "typescript": "5.4.5",
    "typescript-eslint": "7.12.0"
  },
  "packageManager": "yarn@4.2.2"
}
