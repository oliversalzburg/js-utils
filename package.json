{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@oliversalzburg/js-utils",
  "version": "0.2.8",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliversalzburg/js-utils.git"
  },
  "type": "module",
