    "@types/web": "0.0.206",
    "c8": "10.1.3",
    "chai": "5.2.0",
    "lint-staged": "15.4.3",
    "mocha": "11.1.0",
    "typedoc": "0.27.9",
    "typedoc-plugin-mdn-links": "4.0.14",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.6.0"
