{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@oliversalzburg/js-utils",
  "version": "0.0.27",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oliversalzburg/js-utils.git"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./*.js": "./lib/*.js"
  },
  "scripts": {
    "build": "yarn run build:tsc",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc --watch",
    "clean": "rm -rf ./coverage ./lib",
    "docs:typedoc": "typedoc",
    "lint": "yarn run lint:all",
    "lint:all": "yarn run lint:eslint && yarn run lint:prettier && yarn run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "test": "node $(yarn bin mocha) --enable-source-maps",
    "test:coverage": "c8 --reporter html-spa --reporter text node $(yarn bin mocha) --enable-source-maps",
    "test:inspect": "node $(yarn bin mocha) --inspect"
  },
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@droppedcode/typedoc-plugin-copy-assets": "1.0.10",
    "@types/chai": "4.3.11",
    "@types/chai-as-promised": "7.1.8",
    "@types/eslint": "8.56.0",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.5",
    "@types/web": "0.0.129",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "c8": "8.0.1",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "eslint": "8.56.0",
    "eslint-plugin-jsdoc": "46.9.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "lint-staged": "15.2.0",
    "mocha": "10.2.0",
    "prettier": "3.1.1",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "typedoc": "0.25.4",
    "typedoc-plugin-extras": "3.0.0",
    "typedoc-plugin-mdn-links": "3.1.8",
    "typescript": "5.3.3"
  },
  "packageManager": "yarn@4.0.2"
}
